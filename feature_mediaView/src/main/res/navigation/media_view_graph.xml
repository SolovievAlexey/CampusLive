<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/media_view_graph"
    app:startDestination="@id/mediaViewFragment">

    <fragment
        android:id="@+id/mediaViewFragment"
        android:name="ru.campus.feature_mediaview.MediaViewFragment"
        android:label="fragment_media_view"
        tools:layout="@layout/fragment_media_view">

        <argument
            android:name="url"
            app:argType="string" />

        <deepLink app:uri="android-app://ru.campus.live/mediaViewFragment/?url={url}" />

    </fragment>

</navigation>